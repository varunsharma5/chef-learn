#!/bin/bash
echo "Content-type: text/html; charset=iso-8859-1"
echo

echo "<html>"
echo "
  <head>
    <style>

.bubble {
    background-image: url(data:image/png;base64,);
}
    </style>
  </head>
"
echo "<body style='background-color: #87B09A;'>"

echo "<img style='position: absolute; top: 100px;' src='data:image/png;base64,
'>"

echo "<div class='bubble' style='position: absolute; left: 83px; top: 19px; width: 255px; height: 192px; padding: 30 20 0 25; text-overflow: ellipsis; overflow: hidden;'>"

echo "<strong style='font-size: 24px;'>Hello, Habitat!</strong>"

echo "<div style='padding-top: 10px;'>"
echo "The time is <strong>$(date '+%H:%M:%S')</strong> and I live at <strong>$(ifconfig | sed -En 's/127.0.0.1//;s/.*inet (addr:)?(([0-9]*\.){3}[0-9]*).*/\2/p')</strong>."
echo "</div>"

echo "</div>" # bubble

echo "</body>"
echo "</html>"
